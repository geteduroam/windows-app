{
  "App": {
    "GeoApiUrl": "https://geo.eduroam.app/geoip",
    "ProviderApiUrl": "https://discovery.eduroam.app/v1/discovery.json",
    "AppRegistryNamespace": "HKEY_CURRENT_USER\\Software\\geteduroam",
    "CertificateStore": "CurrentUser"
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "${specialfolder:folder=ApplicationData}/geteduroam/geteduroam-${shortdate}.log",
        "layout": "${longdate}|${level:uppercase=true}|${logger}|${message}${onexception:inner=${newline}${exception:format=tostring}}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "logfile"
      }
    ]
  }
}