{
  "App": {
    "GeoApiUrl": "https://geo.eduroam.app/geoip",
    "ProviderApiUrl": "https://getgovroam.nl/v1/discovery.json",
    "AppRegistryNamespace": "HKEY_CURRENT_USER\\Software\\getgovroam"
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "${specialfolder:folder=ApplicationData}/getgovroam/getgovroam-${shortdate}.log"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "logfile"
      }
    ]
  }
}